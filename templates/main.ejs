<header class="navbar navbar-static-top bs-docs-nav" id="top" role="banner">
  <div class="container">
    <div class="navbar-header">
      <button class="navbar-toggle collapsed" type="button" data-toggle="collapse" data-target=".bs-navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a href="../" class="navbar-brand"><% if (typeof(logo) != "undefined") { %><img src="<%=logo%>" alt="logo"/><% } %><% if ( (name.length > 0 && (typeof(logo) == "undefined" ) || nameAndLogo == true)) { %><h3><%=name%></h3><% } %><%  if (name.length == 0 && (typeof(logo) == "undefined")) { %>Home<% } %></a>
    </div>
    <nav class="collapse navbar-collapse bs-navbar-collapse" role="navigation">
    <% if (typeof(menu) != "undefined") { %>
      <ul class="nav navbar-nav">
      <% for (var i = 0; i <= menu.length - 1; i++) { %>

        <li>
          <a href="<%=menu[i].href%>"><%=menu[i].content%></a>
        </li>

      <% } %>
      </ul>
    <% } %>
    <% if (typeof(menuRight) != "undefined") { %>
      <ul class="nav navbar-nav navbar-right nav-pills">
        <% for (var i = 0; i <= menuRight.length - 1; i++) { %>

        <li>
          <a href="<%=menuRight[i].href%>"><%=menuRight[i].content%></a>
        </li>

      <% } %>
      </ul>
    <% } %>
    </nav>
  </div>
</header>
